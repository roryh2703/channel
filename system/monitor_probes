-------------------------------------------------------------------------------
Description
    Writes out values of fields from cells nearest to specified locations.

\*---------------------------------------------------------------------------*/

type            probes;
libs            ("libsampling.so");

writeControl    timeStep;
writeInterval   1;

fields (U);


probeLocations

(

(0   0          0.0376992)           //(0D    0D    pi*D/2)
(0   0.24       0.0376992)           //(0D    10D   pi*D/2)

);



