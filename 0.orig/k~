FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "1";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1.4e-04;

boundaryField
{
    IN
    {
        type                mapped;
        value               uniform 1.4e-04;
        interpolationScheme cell;
        setAverage          false;
        average             1.4e-04;
    }
    OUT
    {
        type            inletOutlet;
        inletValue      $internalField;
    }
    SYM1
    {
        type            slip;
    }
    SYM2
    {
        type            slip;
    }
    periodic1
    {
        type            slip;
    }
    periodic2
    {
        type            slip;
    }
}


// ************************************************************************* //
